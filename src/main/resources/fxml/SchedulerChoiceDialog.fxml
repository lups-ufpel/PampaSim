  </rowConstraints>
   <children>
      <Label text="Algoritmo" />
      <ChoiceBox fx:id="schedulerChoiceBox" prefWidth="150.0" GridPane.columnIndex="1" />
      <Label text="Preempcao" GridPane.rowIndex="1" />
